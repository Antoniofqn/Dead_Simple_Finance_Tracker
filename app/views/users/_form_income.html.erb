<%= simple_form_for(transaction) do |f| %>
  <%= f.input :date %>
  <%= f.input :value %>
  <%= f.input :type_transaction, :as => :hidden, :input_html => { :value => "income"} %>
  <%= f.input :category, collection: ["Salary", "Dividends", "Others"] %>
  <%= f.input :tag %>
  <%= f.submit %>
<% end %>
